{
    "manifest_version": 2,
  
    "name": "Display all used scripts",
    "description": "This extension allows the user to see all the used JS files of the current page.",
    "version": "1.0",
  
    "permissions": [
      "activeTab",
      "storage",
      "tabs"
		],
		
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "Display JS files"
      }
  }